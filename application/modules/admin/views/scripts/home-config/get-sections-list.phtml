<?php
$echoResult = [];
if(isset($this->section) && !empty($this->section)){
    foreach ($this->section as $row) {
        $echoResult[] = array("id" => $row['section'],"text" => $row['section']);
    }
}else{
    $echoResult[] = array("id" => $this->search,'text' => $this->search);
}
$result = array("result" => $echoResult);
echo json_encode($result);
