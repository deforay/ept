<?php

/**
 * AJAX Sample Form Loader
 *
 * Loads the appropriate scheme form partial based on scheme type.
 * Used by index.phtml for the ADD shipment flow.
 */
if ($this->scheme == 'vl') {
	include('partials/schemes/vl.phtml');
} elseif ($this->scheme == 'eid') {
	include('partials/schemes/eid.phtml');
} elseif ($this->scheme == 'dts') {
	include('partials/schemes/dts.phtml');
} elseif ($this->scheme == 'dbs') {
	include('partials/schemes/dbs.phtml');
} elseif ($this->scheme == 'tb') {
	include('partials/schemes/tb.phtml');
} elseif ($this->scheme == 'recency') {
	include('partials/schemes/recency.phtml');
} elseif ($this->scheme == 'covid19') {
	include('partials/schemes/covid19.phtml');
} elseif ($this->scheme == 'generic-test' || $this->userconfig == 'yes') {
	include('partials/schemes/generic.phtml');
}
